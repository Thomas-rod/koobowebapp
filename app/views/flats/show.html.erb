<div class="dashboard-data-set" data-line="appartements">
  <div class="d-flex">
    <%= render partial: 'shared/dashboard', locals: { flats: @flats, schedules: @schedules} %>
    <%= render 'shared/bottom_nav'%>
    <div style=" width:100%;">
      <%= render partial: 'shared/navbar_dashboard', locals: { page_title: 'Appartements', notif_counter: @counter} %>
      <div class="container-disposition">
        <% if @flat.rentings.last.nil? %>
          <%= render partial: 'shared/show_flat/show_not_rented_flat' %>
        <%# elsif %>
          <%#= render partial: 'shared/show_flat/show_ongoing_renting_flat' %>
        <% else %>
          <%= render partial: 'shared/show_flat/show_rented_flat' %>
        <% end %>
      </div>
      <!-- HERE -->
    </div>
  </div>
</div>
