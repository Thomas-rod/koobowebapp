<div class="container-home">
  <section class="container-header-home">
    <%= render partial: 'shared/navbar_home' %>
    <%= image_tag 'flat', id:'flat-banner' %>
    <%= image_tag 'handshake', id:'people-handshake' %>
    <%= image_tag 'square-circle', id:'half-circle' %>
    <div class="inside-banner">
      <h1>Gérer vos bien immobiliers </h1>
      <h2><span id="banner-typed-text"></span></h2>
      <div class="subscribe-mail">
      </div>
    </div>
  </section>
  <section id="section-two">
    <div id="section-two-header">
      <h2><span class="linecoral">Centralisez vos démarches</span></h2>
      <div id="section-card-container">
        <div class="card-section-two">
          <h4><b>Gestion</b></h4>
          <%= image_tag("gestion.png") %>
          <p>Avis d'échéances - Quittances - Charges - Archivage légal</p>
        </div>
        <div class="card-section-two">
          <h4><b>Location</b></h4>
          <%= image_tag("location.png") %>
          <p>Multi-diffusion des annonces - Trouvez votre locataire rapidement - BAIL réglementaire</p>
        </div>
        <div class="card-section-two">
          <h4><b>Simplicité</b></h4>
          <%= image_tag("digit.png") %>
          <p>Réactivité - Traçabilité - Modernité - Signature électronique</p>
        </div>
      </div>
    </div>
  </section>
</div>