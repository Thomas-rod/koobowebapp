<% unless multiple %>
  <% @records.each do |record| %>
    <% unless record.record_documents.select{|rd| rd.document.title == file && rd.document.destroy_by_user == nil }.empty? %>
      <i class="fas fa-check-circle font-green"></i>
    <% else %>
      <i class="fas fa-times"></i>
    <% end %>
  <% end %>
<% else %>
  <% @records.each do |record| %>
    <% unless record.record_documents.select{|rd| rd.document.title == file && rd.document.destroy_by_user == nil && rd.document.docs.count == 3}.empty? %>
      <i class="fas fa-check-circle font-green"></i>
    <% else %>
      <i class="fas fa-times"></i>
    <% end %>
  <% end %>
<% end %>
