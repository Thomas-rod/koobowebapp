<div class="dashboard-data-set" data-line="documents">
  <div class="d-flex">
    <%= render partial: 'shared/dashboard', locals: { flats: @flats, schedules: @schedules, documents: @documents} %>
    <div style=" width:100%;">
      <%= render partial: 'shared/navbar_dashboard', locals: { page_title: 'Documents'}%>
      <div class="container container-document">
        <h2><strong>Vous trouverez ici tous vos documents</strong></h2>
        <%# @flats_rented.each do |flat| %>
        <div class="card-documents">
          <div class="index-doc-left">
            <div class="card-trip card-trip-doc">
              <img src="https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/greece.jpg" id="card-doc-image"/>
            <!--   <div class="card-trip-infos" id="card-info-doc">
                <div>
                  <h2>Title here</h2>
                  <p>Short description here!</p>
                </div>
              </div> -->
            </div>
          </div>
            <div class="index-doc-right">
              <div>
                <h5>Flat.name</h5>
              </div>
              <div class="buttons-document">
                <div class="buttons-doc-left">
                  <%= link_to "Dossier locataire", flats_path, class: "button-doc"  %>
                  <%= link_to "Etat des lieux", flats_path, class: "button-doc"  %>
                </div>
                <div class="buttons-doc-right">
                  <%= link_to "Bail", flats_path, class: "button-doc"  %>
                  <%= link_to "Quittances", flats_path, class: "button-doc"  %>
                </div>
              </div>
            </div>
          </div>
        </div>
        <%# end %>
      </div>
    </div>
  </div>
</div>
